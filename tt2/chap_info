<div class=box>
<table width="99%">
[% USE table(results, rows=5) %]
[% FOREACH column = table.cols %]
[% FOREACH item = column %]
[% IF item == column.first %]
[% class = (class == 'r') ? 's' : 'r' %]
<tr valign="top" class="[% class %]">
[% END %]
<td>
<a href="[% home %]/chapter/[% chapter %]/[% item.subchapter %]">
[% item.subchapter %]</a>
</td>
[% IF item == column.last %]
</tr>
[% END -%]
[% END -%]
[% END -%]
</table>
